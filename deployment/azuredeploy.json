                        },
                        {
                            "name": "ADLoaderCronTimer",
                            "value": "0 30 10 * * *"
                        },
                        {
                            "name": "AzureWebJobs.FakeLoader.Disabled",
                            "value": "true"
                        }
                    ]
                },
                "scmSiteAlsoStopped": false,
                "clientAffinityEnabled": true,
                "clientCertEnabled": false,
                "hostNamesDisabled": false,
                "containerSize": 1536,
